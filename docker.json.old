{
  "experimental": false,
  "default-address-pools": [
    {
      "base": "172.29.0.0/16",
      "size": 22
    }
  ],
  "default-ulimits": {
    "nofile": {
      "Name": "nofile",
      "Hard": 65535,
      "Soft": 65535
    }
  },
  "group": "administrators",
  "log-driver": "json-file",
  "log-opts": {
    "max-size": "10m",
    "max-file": "10"
  },
  "runtimes": {
    "kata-runtime": {
      "path": "/opt/kata/bin/kata-runtime"
    },
    "nvidia-runtime": {
      "path": "/opt/nvidia/bin/nvidia-container-runtime"
    }
  },
  "ip6tables": false
}
